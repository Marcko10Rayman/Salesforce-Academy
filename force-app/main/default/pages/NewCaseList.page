<apex:page controller="NewCaseListController">

    <apex:form >
        <apex:repeat value="{!newCases}" var="case">
        
            <apex:outputLink value="{!case.ID}">
            
                {!case.ID}
            
            </apex:outputLink>
            
            <apex:outputLink value="{!case.CaseNumber}">
            
            	{!case.CaseNumber}
            
            </apex:outputLink>
            
           	
            
        </apex:repeat>
	</apex:form>
</apex:page>